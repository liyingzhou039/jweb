{
	"prefix":"",
	"csses":[
	"css/base.css",
	"css/page-edit.css"
	],
	"jses":[
	],
	"portals":[
		{
			"id":"container",
			"type":"portal.layout.Container",
			"clazz":"container-fluid",
			"children":[
				{
					"id":"row1",
					"type":"portal.layout.Container",
					"clazz":"row",
					"children":[
						{
							"id":"toolbox",
							"type":"portal.page.ToolBar",
							"clazz":"col-sm-2",
							"input":[
								{"id":"service1.success"}
							],
							"output":[
							]
						},
						{
							"id":"pagecontent",
							"type":"portal.page.PageEditor",
							"clazz":"col-sm",
							"input":[
								{"id":"service2.success"}
							],
							"output":[
								{"id":"savePage"}
							]
						}
					]
				}
			]
		}
	],
	"services":[
		{
			"id":"service1",
			"method":"GET",
			"uri":"js/toolbar.json",
			"name":"工具列表",
			"input":[
				{"id":"page._start"}
			],
			"output":[
				{"id":"success"},
				{"id":"error"}
			]
		},
		{
			"id":"service2",
			"method":"GET",
			"uri":"rest/page/getByURI?uri={page.uri}",
			"name":"页面-获取当前页面数据",
			"input":[
				{"id":"page._start"},
				{"id":"service3.success"},
				{"id":"service4.success"}
			],
			"output":[
				{"id":"success"},
				{"id":"error"}
			]
		},
		{
			"id":"service3",
			"method":"POST",
			"uri":"rest/bean/create{beanName}",
			"name":"保存实体",
			"input":[
				{"id":"pagecontent.savePage"}
			],
			"output":[
				{"id":"success"},
				{"id":"error"}
			]
		},
		{
			"id":"service4",
			"method":"PUT",
			"uri":"rest/bean/update{beanName}",
			"name":"更新实体",
			"input":[
				{"id":"pagecontent.updatePage"}
			],
			"output":[
				{"id":"success"},
				{"id":"error"}
			]
		}
	]
}